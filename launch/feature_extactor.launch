<launch>
    

    <node name="kitti_loader" pkg="LIOM" type="kitti_loader" output="screen"> 
        <param name="dataset_folder" type="string" value="/home/libing/data/kitty_raw/" />
        <param name="sequence_number" type="string" value="05" />
        <param name="to_bag" type="bool" value="false" />
        <param name="output_bag_file" type="string" value="/tmp/kitti.bag" /> <!-- replace with your output folder -->
        <param name="fixed_frame_id" type="string" value="odom" />
        <param name="publish_delay" type="int" value="1" />
    </node>

    <node pkg="LIOM" type="lidar_feature_extractor" name="lidar_feature_extractor" output="screen" />
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find LIOM)/rviz_cfg/kitty_velodyne.rviz"/>
</launch>